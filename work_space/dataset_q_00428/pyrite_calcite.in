SOLUTION 1 Pure water
    temp 25
    pH 7 charge
    units mol/kgw
    -water 1
EQUILIBRIUM_PHASES 1
    Pyrite 0 0
    Goethite 0 0
    Calcite 0 0
    CO2(g) -2 10
    Gypsum 0 0
REACTION 1
    O2 1
    0 0.00002 in 101 steps
    NaCl 1
    0 0.1 in 101 steps
INCREMENTAL_REACTIONS true
SELECTED_OUTPUT
    -file selected.csv
    -reset false
    -pH true
    -totals Fe S(6) Ca C(4) Na Cl
    -si Calcite Gypsum
    -equilibrium_phases Pyrite Goethite Calcite CO2(g) Gypsum
END
